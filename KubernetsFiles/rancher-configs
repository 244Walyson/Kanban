
curl https://releases.rancher.com/install-docker/23.0.2.sh | sh

sudo usermod -aG docker $USER


docker run -d --restart=unless-stopped \
  -p 80:80 -p 443:443 \
  --privileged \
  -v /opt/rancher/var/lib:/var/lib/rancher \
  rancher/rancher:latest

rancher password => pMkFCQAZ0JXRtriZ   username -> admin




sudo certbot certonly --manual -d 'waly.dev.br' -d '*.waly.dev.br' -d '*.rancher.waly.dev.br' --agree-tos --no-bootstrap --manual-public-ip-logging-ok --preferred-challenges dns-01 --server https://acme-v02.api.letsencrypt.org/directory

/etc/letsencrypt/live/waly.dev.br/fullchain.pem
/etc/letsencrypt/live/waly.dev.br/privkey.pem

